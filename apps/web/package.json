{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "oxlint --deny-warnings",
    "lint:fix": "oxlint --fix",
    "fmt": "oxfmt",
    "fmt:check": "oxfmt --check",
    "preview": "vite preview",
    "prepare": "husky",
    "postinstall": "if [ -d ../../node_modules/oxlint ]; then rm -rf node_modules/oxlint && cp -R ../../node_modules/oxlint node_modules; fi && if [ -d ../../node_modules/oxfmt ]; then rm -rf node_modules/oxfmt && cp -R ../../node_modules/oxfmt node_modules; fi"
  },
  "dependencies": {
    "@awlt/design": "0.3.0",
    "@base-ui/react": "^1.0.0",
    "@tabler/icons-react": "^3.35.0",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-form": "^1.27.7",
    "@tanstack/react-query": "^5.90.10",
    "clsx": "^2.1.1",
    "facehash": "^0.0.5",
    "lucide-react": "^0.555.0",
    "mp4box": "^2.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-helmet-async": "^2.0.5",
    "react-router": "^7.9.6",
    "tailwindcss": "^4.1.17",
    "zod": "^4.3.6",
    "zustand": "^5.0.8"
